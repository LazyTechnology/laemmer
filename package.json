    "@typescript-eslint/parser": "4.0.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.11.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jest": "^23.16.0",
    "eslint-plugin-jest-dom": "^3.0.1",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-node": "^11.1.0",
