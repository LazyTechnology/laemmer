{
  "name": "payload-nextjs-server",
  "description": "A boilerplate project for NextJS with Payload CMS",
  "keywords": [
    "payload",
    "cms",
    "nextjs"
  ],
  "version": "0.0.1",
  "private": false,
  "license": "MIT",
  "author": "TRBL",
  "scripts": {
    "build:next": "next build",
    "build:server": "tsc --project tsconfig.server.json",
    "build:payload": "payload build",
    "build": "cross-env NODE_ENV=production yarn build:payload && yarn build:server && cross-env NEXT_BUILD=true node dist/index.js",
    "dev": "node dev.js",
    "seed": "node seed/index.js",
    "serve": "cross-env NODE_ENV=production node dist/index.js",
    "postbuild": "next-sitemap"
  },
  "dependencies": {
    "@babel/register": "^7.17.0",
    "@chakra-ui/react": "^2.2.1",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "@loadable/component": "^5.15.2",
    "@react-google-maps/api": "^2.12.0",
    "axios": "^0.27.2",
    "dotenv": "^16.0.0",
    "escape-html": "^1.0.3",
    "express": "^4.18.0",
    "framer-motion": "^6",
    "next": "^12.1.0",
    "next-sitemap": "^3.0.5",
    "node-fetch": "^3.2.6",
    "nprogress": "^0.2.0",
    "payload": "^0.18.0",
    "react": "^18.1.0",
    "react-ga": "^3.3.1",
    "react-google-recaptcha": "^2.1.0",
    "react-icons": "^4.4.0",
    "react-slick": "^0.29.0",
    "sass": "^1.52.0",
    "slate": "^0.81.0",
    "slick-carousel": "^1.8.1",
    "typescript": "^4.7.0",
    "utils": "^0.3.1"
  },
